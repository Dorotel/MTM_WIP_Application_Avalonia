<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:MTM_WIP_Application_Avalonia.ViewModels.MainForm"
             xmlns:materialIcons="using:Material.Icons.Avalonia"
             mc:Ignorable="d"
             d:DesignWidth="1200"
             d:DesignHeight="800"
             x:Class="MTM_WIP_Application_Avalonia.Views.AdvancedRemoveView"
             x:CompileBindings="True"
             x:DataType="vm:AdvancedRemoveViewModel">

	<UserControl.Resources>
		<!-- MTM Design System Resources -->
		<SolidColorBrush x:Key="MTMPrimaryBrush" Color="#6a0dad"/>
		<SolidColorBrush x:Key="MTMSecondaryBrush" Color="#4a0880"/>
		<SolidColorBrush x:Key="MTMAccentBrush" Color="#8a2be2"/>
		<SolidColorBrush x:Key="MTMBorderBrush" Color="#C8C8C8"/>
		<SolidColorBrush x:Key="MTMBackgroundBrush" Color="#F8F8F8"/>
		<SolidColorBrush x:Key="MTMCardBrush" Color="White"/>
		<SolidColorBrush x:Key="MTMHoverBrush" Color="#F0F0F0"/>
		<SolidColorBrush x:Key="MTMPressedBrush" Color="#E0E0E0"/>
		<SolidColorBrush x:Key="MTMSuccessBrush" Color="#28a745"/>
		<SolidColorBrush x:Key="MTMWarningBrush" Color="#ffc107"/>
		<SolidColorBrush x:Key="MTMDangerBrush" Color="#dc3545"/>
		
		<!-- Avalonia Linear Gradient Brushes -->
		<LinearGradientBrush x:Key="MTMVerticalGradientBrush" StartPoint="0,0" EndPoint="0,1">
			<GradientStop Color="#6a0dad" Offset="0"/>
			<GradientStop Color="#4a0880" Offset="1"/>
		</LinearGradientBrush>
		
		<LinearGradientBrush x:Key="MTMHorizontalGradientBrush" StartPoint="0,0" EndPoint="1,0">
			<GradientStop Color="#6a0dad" Offset="0"/>
			<GradientStop Color="#8a2be2" Offset="1"/>
		</LinearGradientBrush>
	</UserControl.Resources>

	<UserControl.Styles>
		<!-- Compact Button Styles -->
		<Style Selector="Button.MTMPrimaryButton">
			<Setter Property="Background" Value="{DynamicResource MTMPrimaryBrush}"/>
			<Setter Property="Foreground" Value="White"/>
			<Setter Property="BorderBrush" Value="{DynamicResource MTMPrimaryBrush}"/>
			<Setter Property="BorderThickness" Value="1"/>
			<Setter Property="CornerRadius" Value="4"/>
			<Setter Property="Padding" Value="8,4"/>
			<Setter Property="FontWeight" Value="Medium"/>
			<Setter Property="FontSize" Value="10"/>
			<Setter Property="MinHeight" Value="28"/>
			<Setter Property="Cursor" Value="Hand"/>
		</Style>
		<Style Selector="Button.MTMPrimaryButton:pointerover">
			<Setter Property="Background" Value="{DynamicResource MTMSecondaryBrush}"/>
		</Style>

		<Style Selector="Button.MTMSecondaryButton">
			<Setter Property="Background" Value="White"/>
			<Setter Property="Foreground" Value="{DynamicResource MTMPrimaryBrush}"/>
			<Setter Property="BorderBrush" Value="{DynamicResource MTMPrimaryBrush}"/>
			<Setter Property="BorderThickness" Value="1"/>
			<Setter Property="CornerRadius" Value="4"/>
			<Setter Property="Padding" Value="8,4"/>
			<Setter Property="FontWeight" Value="Medium"/>
			<Setter Property="FontSize" Value="10"/>
			<Setter Property="MinHeight" Value="28"/>
			<Setter Property="Cursor" Value="Hand"/>
		</Style>
		<Style Selector="Button.MTMSecondaryButton:pointerover">
			<Setter Property="Background" Value="{DynamicResource MTMHoverBrush}"/>
		</Style>

		<Style Selector="Button.MTMDangerButton">
			<Setter Property="Background" Value="{DynamicResource MTMDangerBrush}"/>
			<Setter Property="Foreground" Value="White"/>
			<Setter Property="BorderBrush" Value="{DynamicResource MTMDangerBrush}"/>
			<Setter Property="BorderThickness" Value="1"/>
			<Setter Property="CornerRadius" Value="4"/>
			<Setter Property="Padding" Value="8,4"/>
			<Setter Property="FontWeight" Value="Medium"/>
			<Setter Property="FontSize" Value="10"/>
			<Setter Property="MinHeight" Value="28"/>
			<Setter Property="Cursor" Value="Hand"/>
		</Style>

		<!-- Compact Input Styles -->
		<Style Selector="TextBox.MTMTextBox">
			<Setter Property="BorderBrush" Value="{DynamicResource MTMBorderBrush}"/>
			<Setter Property="BorderThickness" Value="1"/>
			<Setter Property="CornerRadius" Value="4"/>
			<Setter Property="Padding" Value="6,4"/>
			<Setter Property="Background" Value="White"/>
			<Setter Property="FontSize" Value="10"/>
			<Setter Property="MinHeight" Value="26"/>
		</Style>

		<Style Selector="AutoCompleteBox.MTMAutoCompleteBox">
			<Setter Property="BorderBrush" Value="{DynamicResource MTMBorderBrush}"/>
			<Setter Property="BorderThickness" Value="1"/>
			<Setter Property="CornerRadius" Value="4"/>
			<Setter Property="Padding" Value="6,4"/>
			<Setter Property="Background" Value="White"/>
			<Setter Property="FontSize" Value="10"/>
			<Setter Property="MinHeight" Value="26"/>
		</Style>
	</UserControl.Styles>

	<!-- Main Grid with optimized row definitions -->
	<Grid RowDefinitions="Auto,*,Auto" Margin="8" Background="{DynamicResource MTMBackgroundBrush}">
		
		<!-- Compact Title Section -->
		<Border Grid.Row="0" 
				Background="{DynamicResource MTMCardBrush}"
				BorderBrush="{DynamicResource MTMBorderBrush}"
				BorderThickness="1"
				CornerRadius="6"
				Padding="12,8"
				Margin="0,0,0,8">
			<Grid ColumnDefinitions="Auto,*,Auto">
				<materialIcons:MaterialIcon Grid.Column="0"
											Kind="DeleteSweep"
											Width="20"
											Height="20"
											Foreground="{DynamicResource MTMPrimaryBrush}"
											VerticalAlignment="Center"
											Margin="0,0,8,0"/>
				<TextBlock Grid.Column="1"
						   Text="Advanced Inventory Removal"
						   FontSize="16"
						   FontWeight="SemiBold"
						   Foreground="{DynamicResource MTMPrimaryBrush}"
						   VerticalAlignment="Center"/>
				<TextBlock Grid.Column="2"
						   Text="Enhanced removal operations with audit tracking"
						   FontSize="10"
						   Foreground="#666666"
						   VerticalAlignment="Center"/>
			</Grid>
		</Border>

		<!-- Optimized Main Panel -->
		<Border Grid.Row="1"
				Background="{DynamicResource MTMCardBrush}"
				BorderBrush="{DynamicResource MTMBorderBrush}"
				BorderThickness="1"
				CornerRadius="6"
				Padding="8">
			<Grid ColumnSpacing="8">
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="{Binding FilterPanelWidth}"/>
					<ColumnDefinition Width="*"/>
				</Grid.ColumnDefinitions>
				
				<!-- Compact Left Panel: Search Criteria -->
				<Border Grid.Column="0"
						Background="{DynamicResource MTMCardBrush}"
						BorderBrush="{DynamicResource MTMBorderBrush}"
						BorderThickness="1"
						CornerRadius="6">
					<Grid ColumnDefinitions="Auto,*">
						<!-- Compact Vertical Header Bar -->
						<Border Grid.Column="0"
								Background="{StaticResource MTMVerticalGradientBrush}"
								BorderBrush="{DynamicResource MTMBorderBrush}"
								BorderThickness="0,0,1,0"
								Width="40"
								CornerRadius="6,0,0,6">
							<Grid RowDefinitions="*,Auto,Auto,*">
								<!-- Filter Icon -->
								<materialIcons:MaterialIcon Grid.Row="1"
															Kind="FilterVariant"
															Width="16"
															Height="16"
															Foreground="White"
															HorizontalAlignment="Center"
															Margin="0,0,0,4"/>
								
								<!-- Toggle Button -->
								<Button Grid.Row="2"
										Command="{Binding ToggleFilterPanelCommand}"
										Width="24"
										Height="24"
										Background="White"
										Foreground="{DynamicResource MTMPrimaryBrush}"
										BorderThickness="1"
										BorderBrush="White"
										CornerRadius="12"
										HorizontalAlignment="Center"
										VerticalAlignment="Center"
										Margin="8,0,8,8"
										Cursor="Hand"
										ToolTip.Tip="Toggle Filter Panel">
									<materialIcons:MaterialIcon Kind="{Binding CollapseButtonIcon}"
																Width="12"
																Height="12"/>
								</Button>
							</Grid>
						</Border>

						<!-- Compact Filter Content -->
						<Border Grid.Column="1"
								IsVisible="{Binding IsFilterPanelExpanded}"
								Padding="8,6">
							<StackPanel Spacing="8">
								
								<!-- Part ID -->
								<StackPanel>
									<Grid ColumnDefinitions="Auto,*" ColumnSpacing="4" Margin="0,0,0,2">
										<materialIcons:MaterialIcon Grid.Column="0"
																	Kind="Identifier"
																	Width="12"
																	Height="12"
																	Foreground="{DynamicResource MTMPrimaryBrush}"
																	VerticalAlignment="Center"/>
										<TextBlock Grid.Column="1" 
												   Text="Part ID:" 
												   FontSize="10" 
												   FontWeight="Medium" 
												   Foreground="{DynamicResource MTMPrimaryBrush}"/>
									</Grid>
									<AutoCompleteBox ItemsSource="{Binding PartIDOptions}"
													 Text="{Binding FilterPartIDText, Mode=TwoWay}"
													 Classes="MTMAutoCompleteBox"
													 HorizontalAlignment="Stretch"/>
								</StackPanel>

								<!-- Location -->
								<StackPanel>
									<Grid ColumnDefinitions="Auto,*" ColumnSpacing="4" Margin="0,0,0,2">
										<materialIcons:MaterialIcon Grid.Column="0"
																	Kind="MapMarker"
																	Width="12"
																	Height="12"
																	Foreground="{DynamicResource MTMPrimaryBrush}"
																	VerticalAlignment="Center"/>
										<TextBlock Grid.Column="1" 
												   Text="Location:" 
												   FontSize="10" 
												   FontWeight="Medium" 
												   Foreground="{DynamicResource MTMPrimaryBrush}"/>
									</Grid>
									<AutoCompleteBox ItemsSource="{Binding LocationOptions}"
													 Text="{Binding FilterLocationText, Mode=TwoWay}"
													 Classes="MTMAutoCompleteBox"
													 HorizontalAlignment="Stretch"/>
								</StackPanel>

								<!-- Operation -->
								<StackPanel>
									<Grid ColumnDefinitions="Auto,*" ColumnSpacing="4" Margin="0,0,0,2">
										<materialIcons:MaterialIcon Grid.Column="0"
																	Kind="Cog"
																	Width="12"
																	Height="12"
																	Foreground="{DynamicResource MTMPrimaryBrush}"
																	VerticalAlignment="Center"/>
										<TextBlock Grid.Column="1" 
												   Text="Operation:" 
												   FontSize="10" 
												   FontWeight="Medium" 
												   Foreground="{DynamicResource MTMPrimaryBrush}"/>
									</Grid>
									<AutoCompleteBox ItemsSource="{Binding OperationOptions}"
													 Text="{Binding FilterOperation, Mode=TwoWay}"
													 Classes="MTMAutoCompleteBox"
													 HorizontalAlignment="Stretch"/>
								</StackPanel>

								<!-- User -->
								<StackPanel>
									<Grid ColumnDefinitions="Auto,*" ColumnSpacing="4" Margin="0,0,0,2">
										<materialIcons:MaterialIcon Grid.Column="0"
																	Kind="Account"
																	Width="12"
																	Height="12"
																	Foreground="{DynamicResource MTMPrimaryBrush}"
																	VerticalAlignment="Center"/>
										<TextBlock Grid.Column="1" 
												   Text="User:" 
												   FontSize="10" 
												   FontWeight="Medium" 
												   Foreground="{DynamicResource MTMPrimaryBrush}"/>
									</Grid>
									<AutoCompleteBox ItemsSource="{Binding UserOptions}"
													 Text="{Binding FilterUserText, Mode=TwoWay}"
													 Watermark="Enter User"
													 Classes="MTMAutoCompleteBox"
													 HorizontalAlignment="Stretch"/>
								</StackPanel>

								<!-- Notes -->
								<StackPanel>
									<Grid ColumnDefinitions="Auto,*" ColumnSpacing="4" Margin="0,0,0,2">
										<materialIcons:MaterialIcon Grid.Column="0"
																	Kind="NoteText"
																	Width="12"
																	Height="12"
																	Foreground="{DynamicResource MTMPrimaryBrush}"
																	VerticalAlignment="Center"/>
										<TextBlock Grid.Column="1" 
												   Text="Notes:" 
												   FontSize="10" 
												   FontWeight="Medium" 
												   Foreground="{DynamicResource MTMPrimaryBrush}"/>
									</Grid>
									<TextBox Text="{Binding FilterNotes, Mode=TwoWay}"
											 Classes="MTMTextBox"
											 HorizontalAlignment="Stretch"/>
								</StackPanel>

								<!-- Quantity Range -->
								<StackPanel>
									<Grid ColumnDefinitions="Auto,*" ColumnSpacing="4" Margin="0,0,0,2">
										<materialIcons:MaterialIcon Grid.Column="0"
																	Kind="Counter"
																	Width="12"
																	Height="12"
																	Foreground="{DynamicResource MTMPrimaryBrush}"
																	VerticalAlignment="Center"/>
										<TextBlock Grid.Column="1" 
												   Text="Quantity:" 
												   FontSize="10" 
												   FontWeight="Medium" 
												   Foreground="{DynamicResource MTMPrimaryBrush}"/>
									</Grid>
									<Grid ColumnDefinitions="*,4,*" ColumnSpacing="4">
										<TextBox Grid.Column="0" 
												 Watermark="Min"
												 Text="{Binding QuantityMin, Mode=TwoWay}"
												 Classes="MTMTextBox"
												 HorizontalAlignment="Stretch"/>
										<TextBox Grid.Column="2" 
												 Watermark="Max"
												 Text="{Binding QuantityMax, Mode=TwoWay}"
												 Classes="MTMTextBox"
												 HorizontalAlignment="Stretch"/>
									</Grid>
								</StackPanel>

								<!-- Compact Date Range -->
								<StackPanel>
									<CheckBox IsChecked="{Binding UseDateRange, Mode=TwoWay}"
											  FontSize="10"
											  FontWeight="Medium"
											  Foreground="{DynamicResource MTMPrimaryBrush}"
											  Margin="0,0,0,4">
										<CheckBox.Content>
											<Grid ColumnDefinitions="Auto,*" ColumnSpacing="4">
												<materialIcons:MaterialIcon Grid.Column="0"
																			Kind="CalendarRange"
																			Width="12"
																			Height="12"
																			Foreground="{DynamicResource MTMPrimaryBrush}"
																			VerticalAlignment="Center"/>
												<TextBlock Grid.Column="1" 
														   Text="Date Range:" 
														   FontSize="10" 
														   FontWeight="Medium"/>
											</Grid>
										</CheckBox.Content>
									</CheckBox>
									<Grid ColumnDefinitions="*,4,*" 
										  ColumnSpacing="4"
										  IsEnabled="{Binding UseDateRange}">
										<!-- Start Date with Click Handler -->
										<Border Grid.Column="0"
												BorderBrush="{DynamicResource MTMBorderBrush}"
												BorderThickness="1"
												CornerRadius="4"
												Background="White"
												Cursor="Hand">
											<Grid>
												<TextBox x:Name="StartDateTextBox"
														 Text="{Binding RemovalDateRangeStart, StringFormat='{}{0:MM/dd/yyyy}', Mode=OneWay}"
														 IsReadOnly="True"
														 Classes="MTMTextBox"
														 Background="Transparent"
														 BorderThickness="0"
														 Watermark="Start Date"
														 HorizontalAlignment="Stretch"
														 Padding="6,4,24,4"/>
												<!-- Calendar Icon -->
												<materialIcons:MaterialIcon Kind="Calendar"
																			Width="12"
																			Height="12"
																			Foreground="{DynamicResource MTMPrimaryBrush}"
																			HorizontalAlignment="Right"
																			VerticalAlignment="Center"
																			Margin="0,0,6,0"
																			IsHitTestVisible="False"/>
												<!-- Invisible Button for Click Handling -->
												<Button Background="Transparent"
														BorderThickness="0"
														HorizontalAlignment="Stretch"
														VerticalAlignment="Stretch"
														Cursor="Hand"
														Padding="0"
														Margin="0"
														x:Name="StartDateButton">
													<Button.Flyout>
														<Flyout Placement="BottomEdgeAlignedLeft">
															<Border Background="White"
																	BorderBrush="{DynamicResource MTMBorderBrush}"
																	BorderThickness="1"
																	CornerRadius="6"
																	Padding="12">
																<StackPanel Spacing="8">
																	<Grid ColumnDefinitions="Auto,*" ColumnSpacing="6">
																		<materialIcons:MaterialIcon Grid.Column="0"
																									Kind="CalendarStart"
																									Width="14"
																									Height="14"
																									Foreground="{DynamicResource MTMPrimaryBrush}"
																									VerticalAlignment="Center"/>
																		<TextBlock Grid.Column="1"
																				   Text="Select Start Date"
																				   FontSize="11"
																				   FontWeight="Medium"
																				   Foreground="{DynamicResource MTMPrimaryBrush}"/>
																	</Grid>
																	<DatePicker SelectedDate="{Binding RemovalDateRangeStart, Mode=TwoWay}"
																				FontSize="10"
																				MinWidth="180"/>
																</StackPanel>
															</Border>
														</Flyout>
													</Button.Flyout>
												</Button>
											</Grid>
										</Border>
										
										<!-- End Date with Click Handler -->
										<Border Grid.Column="2"
												BorderBrush="{DynamicResource MTMBorderBrush}"
												BorderThickness="1"
												CornerRadius="4"
												Background="White"
												Cursor="Hand">
											<Grid>
												<TextBox x:Name="EndDateTextBox"
														 Text="{Binding RemovalDateRangeEnd, StringFormat='{}{0:MM/dd/yyyy}', Mode=OneWay}"
														 IsReadOnly="True"
														 Classes="MTMTextBox"
														 Background="Transparent"
														 BorderThickness="0"
														 Watermark="End Date"
														 HorizontalAlignment="Stretch"
															Padding="6,4,24,4"/>
												<!-- Calendar Icon -->
												<materialIcons:MaterialIcon Kind="Calendar"
																			Width="12"
																			Height="12"
																			Foreground="{DynamicResource MTMPrimaryBrush}"
																			HorizontalAlignment="Right"
																			VerticalAlignment="Center"
																			Margin="0,0,6,0"
																			IsHitTestVisible="False"/>
												<!-- Invisible Button for Click Handling -->
												<Button Background="Transparent"
														BorderThickness="0"
														HorizontalAlignment="Stretch"
														VerticalAlignment="Stretch"
														Cursor="Hand"
														Padding="0"
														Margin="0"
														x:Name="EndDateButton">
													<Button.Flyout>
														<Flyout Placement="BottomEdgeAlignedLeft">
															<Border Background="White"
																	BorderBrush="{DynamicResource MTMBorderBrush}"
																	BorderThickness="1"
																	CornerRadius="6"
																	Padding="12">
																<StackPanel Spacing="8">
																	<Grid ColumnDefinitions="Auto,*" ColumnSpacing="6">
																		<materialIcons:MaterialIcon Grid.Column="0"
																									Kind="CalendarEnd"
																									Width="14"
																									Height="14"
																									Foreground="{DynamicResource MTMPrimaryBrush}"
																									VerticalAlignment="Center"/>
																		<TextBlock Grid.Column="1"
																				   Text="Select End Date"
																				   FontSize="11"
																				   FontWeight="Medium"
																				   Foreground="{DynamicResource MTMPrimaryBrush}"/>
																	</Grid>
																	<DatePicker SelectedDate="{Binding RemovalDateRangeEnd, Mode=TwoWay}"
																				FontSize="10"
																				MinWidth="180"/>
																</StackPanel>
															</Border>
														</Flyout>
													</Button.Flyout>
												</Button>
											</Grid>
										</Border>
									</Grid>
								</StackPanel>

							</StackPanel>
						</Border>
					</Grid>
				</Border>

				<!-- Right Panel: Removal History Grid -->
				<Border Grid.Column="1"
						Background="{DynamicResource MTMCardBrush}"
						BorderBrush="{DynamicResource MTMBorderBrush}"
						BorderThickness="1"
						CornerRadius="6"
						Padding="6">
					<Grid RowDefinitions="Auto,*">
						<!-- Compact Grid Header -->
						<Border Grid.Row="0" 
								Background="{StaticResource MTMHorizontalGradientBrush}"
								CornerRadius="4"
								Padding="8,6"
								Margin="0,0,0,6">
							<Grid ColumnDefinitions="Auto,*,Auto">
								<materialIcons:MaterialIcon Grid.Column="0"
															Kind="History"
															Width="16"
															Height="16"
															Foreground="White"
															VerticalAlignment="Center"
															Margin="0,0,6,0"/>
								<TextBlock Grid.Column="1"
										   Text="Removal History"
										   FontSize="12"
										   FontWeight="SemiBold"
										   Foreground="White"
										   VerticalAlignment="Center"/>
								<StackPanel Grid.Column="2" 
											Orientation="Horizontal" 
											Spacing="4">
									<TextBlock Text="{Binding RemovalHistoryGrid.Count}"
											   FontSize="11"
											   FontWeight="Bold"
											   Foreground="White"
											   VerticalAlignment="Center"/>
									<TextBlock Text="records"
											   FontSize="10"
											   Foreground="White"
											   VerticalAlignment="Center"/>
								</StackPanel>
							</Grid>
						</Border>
						
						<!-- Compact Data Grid -->
						<DataGrid Grid.Row="1"
								  ItemsSource="{Binding RemovalHistoryGrid}"
								  SelectedItem="{Binding SelectedHistoryItem, Mode=TwoWay}"
								  AutoGenerateColumns="False"
								  IsReadOnly="True"
								  GridLinesVisibility="Horizontal"
								  HeadersVisibility="Column"
								  CanUserResizeColumns="True"
								  CanUserSortColumns="True"
								  Background="White"
								  RowBackground="White"
								  BorderBrush="{DynamicResource MTMBorderBrush}"
								  BorderThickness="1"
								  CornerRadius="4"
								  FontSize="9"
								  RowHeight="22">
							<DataGrid.Columns>
								<DataGridTextColumn Header="ID" Binding="{Binding ID}" Width="40"/>
								<DataGridTextColumn Header="Part" Binding="{Binding PartID}" Width="100"/>
								<DataGridTextColumn Header="Op" Binding="{Binding Operation}" Width="40"/>
								<DataGridTextColumn Header="Location" Binding="{Binding Location}" Width="70"/>
								<DataGridTextColumn Header="Qty" Binding="{Binding Quantity}" Width="50"/>
								<DataGridTextColumn Header="User" Binding="{Binding User}" Width="60"/>
								<DataGridTextColumn Header="Date" Binding="{Binding DateRemoved, StringFormat='{}{0:MM/dd/yy}'}" Width="60"/>
							</DataGrid.Columns>
						</DataGrid>
					</Grid>
				</Border>

			</Grid>
		</Border>

		<!-- Compact Bottom Action Panel -->
		<Border Grid.Row="2"
				Background="{DynamicResource MTMCardBrush}"
				BorderBrush="{DynamicResource MTMBorderBrush}"
				BorderThickness="1"
				CornerRadius="6"
				Padding="8"
				Margin="0,8,0,0">
			<Grid ColumnDefinitions="*,Auto">
				<!-- Left side buttons -->
				<StackPanel Grid.Column="0" 
							Orientation="Horizontal" 
							HorizontalAlignment="Left" 
							Spacing="6">
					<Button Classes="MTMPrimaryButton"
							Width="70" 
							Height="28"
							Command="{Binding SearchCommand}">
						<StackPanel Orientation="Horizontal" Spacing="4">
							<materialIcons:MaterialIcon Kind="Magnify" Width="12" Height="12"/>
							<TextBlock Text="Search" FontSize="10"/>
						</StackPanel>
					</Button>
					<Button Classes="MTMSecondaryButton"
							Width="70" 
							Height="28"
							Command="{Binding UndoRemovalCommand}"
							IsEnabled="{Binding CanUndo}">
						<StackPanel Orientation="Horizontal" Spacing="4">
							<materialIcons:MaterialIcon Kind="Undo" Width="12" Height="12"/>
							<TextBlock Text="Undo" FontSize="10"/>
						</StackPanel>
					</Button>
					<Button Classes="MTMDangerButton"
							Width="70" 
							Height="28"
							Command="{Binding RemoveSelectedCommand}">
						<StackPanel Orientation="Horizontal" Spacing="4">
							<materialIcons:MaterialIcon Kind="Delete" Width="12" Height="12"/>
							<TextBlock Text="Delete" FontSize="10"/>
						</StackPanel>
					</Button>
					<Button Classes="MTMSecondaryButton"
							Width="70" 
							Height="28"
							Command="{Binding ClearCommand}">
						<StackPanel Orientation="Horizontal" Spacing="4">
							<materialIcons:MaterialIcon Kind="Refresh" Width="12" Height="12"/>
							<TextBlock Text="Reset" FontSize="10"/>
						</StackPanel>
					</Button>
					<Button Classes="MTMSecondaryButton"
							Width="70" 
							Height="28"
							Command="{Binding PrintRemovalSummaryCommand}">
						<StackPanel Orientation="Horizontal" Spacing="4">
							<materialIcons:MaterialIcon Kind="Printer" Width="12" Height="12"/>
							<TextBlock Text="Print" FontSize="10"/>
						</StackPanel>
					</Button>
				</StackPanel>

				<!-- Right side button -->
				<Button Grid.Column="1"
						Classes="MTMSecondaryButton"
						Width="120" 
						Height="28"
						Command="{Binding BackToNormalCommand}">
					<StackPanel Orientation="Horizontal" Spacing="6">
						<materialIcons:MaterialIcon Kind="ArrowLeft" Width="12" Height="12"/>
						<TextBlock Text="Back to Normal" FontSize="10"/>
					</StackPanel>
				</Button>
			</Grid>
		</Border>
	</Grid>
</UserControl>