<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <!-- Force ReactiveUI to use the correct version -->
      <dependentAssembly>
        <assemblyIdentity name="ReactiveUI" publicKeyToken="552dd7e47bc69ba8" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-20.1.63.0" newVersion="20.1.63.0" />
      </dependentAssembly>
      
      <!-- Force System.Reactive to use the correct version -->
      <dependentAssembly>
        <assemblyIdentity name="System.Reactive" publicKeyToken="94bc3704cddfc263" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.1.0" newVersion="6.0.1.0" />
      </dependentAssembly>
      
      <!-- Prevent loading of ReactiveUI.XamForms -->
      <dependentAssembly>
        <assemblyIdentity name="ReactiveUI.XamForms" publicKeyToken="552dd7e47bc69ba8" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-20.1.63.0" newVersion="20.1.63.0" />
        <codeBase version="20.1.63.0" href="ReactiveUI.dll" />
      </dependentAssembly>
      
      <!-- Additional Avalonia-related bindings -->
      <dependentAssembly>
        <assemblyIdentity name="Avalonia.Base" publicKeyToken="c8d484a7012f9a8b" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.3.4.0" newVersion="11.3.4.0" />
      </dependentAssembly>
      
      <dependentAssembly>
        <assemblyIdentity name="Avalonia.Controls" publicKeyToken="c8d484a7012f9a8b" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.3.4.0" newVersion="11.3.4.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
