<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- ================================================================== -->
	<!-- MTM Light Rose Default Theme - Maximum Robustness Edition        -->
	<!-- Complete premium theme with all visual effects and interactions   -->
	<!-- ================================================================== -->

	<!-- Core MTM Light Rose Default Brand Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryAction" Color="#C2185B"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryAction" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Warning" Color="#E65100"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Status" Color="#880E4F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Critical" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Highlight" Color="#C2185B"/>

	<!-- Extended Palette for Enhanced Visual Effects -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.DarkNavigation" Color="#880E4F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.CardBackground" Color="#FCE4EC"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.HoverBackground" Color="#FFF0F5"/>

	<!-- Advanced Interactive State Colors -->
	<!-- Overlay Text Brush for Universal Contrast -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.OverlayTextBrush" Color="#000000"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryHoverBrush" Color="#C2185B"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryHoverBrush" Color="#106EBE"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.MagentaHoverBrush" Color="#C2185B"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryPressedBrush" Color="#880E4F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryPressedBrush" Color="#005A9E"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.MagentaPressedBrush" Color="#C2185B"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDisabledBrush" Color="#F8BBD9"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryDisabledBrush" Color="#FCE4EC"/>

	<!-- Premium Gradient Brushes for Visual Excellence -->
	<LinearGradientBrush x:Key="MTM_Shared_Logic.PrimaryGradientBrush" StartPoint="0,0" EndPoint="1,0">
		<GradientStop Color="#AD1457" Offset="0"/>
		<GradientStop Color="#0078D4" Offset="0.5"/>
		<GradientStop Color="#C2185B" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="MTM_Shared_Logic.HeroGradientBrush" StartPoint="0,0" EndPoint="1,1">
		<GradientStop Color="#880E4F" Offset="0"/>
		<GradientStop Color="#AD1457" Offset="0.3"/>
		<GradientStop Color="#0078D4" Offset="0.7"/>
		<GradientStop Color="#C2185B" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="MTM_Shared_Logic.SidebarGradientBrush" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#880E4F" Offset="0"/>
		<GradientStop Color="#AD1457" Offset="0.6"/>
		<GradientStop Color="#C2185B" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="MTM_Shared_Logic.CardHoverGradientBrush" StartPoint="0,0" EndPoint="1,0">
		<GradientStop Color="#FFF0F5" Offset="0"/>
		<GradientStop Color="#FCE4EC" Offset="0.5"/>
		<GradientStop Color="#F8BBD9" Offset="1" />
	</LinearGradientBrush>

	<RadialGradientBrush x:Key="MTM_Shared_Logic.AccentRadialBrush" Center="0.5,0.5" RadiusX="100%" RadiusY="100%">
		<GradientStop Color="#C2185B" Offset="0"/>
		<GradientStop Color="#AD1457" Offset="0.7"/>
		<GradientStop Color="#880E4F" Offset="1"/>
	</RadialGradientBrush>

	<!-- Comprehensive UI Layout Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.MainBackground" Color="#FAFAFA"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ContentAreas" Color="#FFFFFF"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SidebarDark" Color="#880E4F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PageHeaders" Color="#AD1457"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.FooterBackgroundBrush" Color="#880E4F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.StatusBarBackgroundBrush" Color="#880E4F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.CardBackgroundBrush" Color="#FFFFFF"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PanelBackgroundBrush" Color="#F5F5F5"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderBrush" Color="#FCE4EC"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderDarkBrush" Color="#F8BBD9"/>

	<!-- Advanced Border and Shadow Effects -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderLightBrush" Color="#FFF0F5"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderAccentBrush" Color="#F48FB1"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ShadowBrush" Color="#33000000"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.DropShadowBrush" Color="#66880E4F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.GlowBrush" Color="#80E91E63"/>

	<!-- Comprehensive Text Color System -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.HeadingText" Color="#880E4F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BodyText" Color="#424242"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.TertiaryTextBrush" Color="#666666"/>
	
	<SolidColorBrush x:Key="MTM_Shared_Logic.InteractiveText" Color="#AD1457"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.LinkTextBrush" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.MutedTextBrush" Color="#BDBDBD"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.HighlightTextBrush" Color="#C2185B"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PlaceholderTextBrush" Color="#9E9E9E"/>

	<!-- Enhanced Semantic Colors with Variants (KEEP EXACT COLORS) -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessBrush" Color="#2E7D32"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessLightBrush" Color="#C8E6C9"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessDarkBrush" Color="#2E7D32"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.WarningBrush" Color="#E65100"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.WarningLightBrush" Color="#FFE0B2"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.WarningDarkBrush" Color="#F57C00"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorBrush" Color="#F44336"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorLightBrush" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorDarkBrush" Color="#D32F2F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InfoBrush" Color="#2196F3"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InfoLightBrush" Color="#BBDEFB"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InfoDarkBrush" Color="#1976D2"/>
	<!-- Transaction Type Colors for History Panel (WCAG 2.1 AA Compliant) -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionInBrush" Color="#198754"/>      <!-- Green for IN - 4.5:1 contrast -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionOutBrush" Color="#DC3545"/>     <!-- Red for OUT - 4.5:1 contrast -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionTransferBrush" Color="#E65100"/> <!-- Orange for Transfer - 4.5:1 contrast -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionInLightBrush" Color="#D1E7DD"/>  <!-- Light Green -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionOutLightBrush" Color="#F8D7DA"/> <!-- Light Red -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionTransferLightBrush" Color="#FFF3CD"/> <!-- Light Orange -->



	<!-- Specialized Theme Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized1" Color="#AD1457"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized2" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized3" Color="#C2185B"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized4" Color="#880E4F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized5" Color="#C2185B"/>

	<!-- Advanced State Management Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.FocusBrush" Color="#AD1457"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SelectionBrush" Color="#80F8BBD9"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ActiveBrush" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InactiveBrush" Color="#BDBDBD"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.LoadingBrush" Color="#F8BBD9"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ProcessingBrush" Color="#C2185B"/>


<SolidColorBrush x:Key="MTM_Shared_Logic.InputBackground" Color="#FFFFFF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InputBorder" Color="#CED4DA"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InputFocusBorder" Color="#0056B3"/>
</ResourceDictionary>


