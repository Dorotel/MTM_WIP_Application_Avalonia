<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- ================================================================== -->
    <!-- MTM WIP Application Purple Brand Theme System                     -->
    <!-- ================================================================== -->
    <!-- Generated: December 19, 2024                                      -->
    <!-- Version: 1.0 - Complete Theme Implementation                      -->
    <!-- ================================================================== -->

    <!-- ================================================================== -->
    <!-- PRIMARY MTM COLOR PALETTE                                         -->
    <!-- ================================================================== -->
    
    <!-- Core MTM Brand Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryBrush" Color="#4B45ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryBrush" Color="#8345ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.MagentaAccentBrush" Color="#BA45ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.BlueAccentBrush" Color="#4574ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PinkAccentBrush" Color="#ED45E7"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.LightPurpleBrush" Color="#B594ED"/>
    
    <!-- Extended Palette -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDarkBrush" Color="#2D1B69"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryLightBrush" Color="#E8E5FF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryExtraLightBrush" Color="#F8F7FF"/>
    
    <!-- ================================================================== -->
    <!-- INTERACTIVE STATE COLORS                                          -->
    <!-- ================================================================== -->
    
    <!-- Hover States -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryHoverBrush" Color="#5A51F0"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryHoverBrush" Color="#9354F0"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.MagentaHoverBrush" Color="#C754F0"/>
    
    <!-- Pressed States -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryPressedBrush" Color="#3B35E0"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryPressedBrush" Color="#7235E0"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.MagentaPressedBrush" Color="#A935E0"/>
    
    <!-- Disabled States -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDisabledBrush" Color="#B594ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryDisabledBrush" Color="#D4C4F7"/>
    
    <!-- ================================================================== -->
    <!-- GRADIENT BRUSHES                                                   -->
    <!-- ================================================================== -->
    
    <!-- Primary MTM Gradient (Horizontal) -->
    <LinearGradientBrush x:Key="MTM_Shared_Logic.PrimaryGradientBrush" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#4B45ED" Offset="0"/>
        <GradientStop Color="#8345ED" Offset="0.5"/>
        <GradientStop Color="#BA45ED" Offset="1"/>
    </LinearGradientBrush>
    
    <!-- Hero Section Gradient (Diagonal) -->
    <LinearGradientBrush x:Key="MTM_Shared_Logic.HeroGradientBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#4574ED" Offset="0"/>
        <GradientStop Color="#4B45ED" Offset="0.3"/>
        <GradientStop Color="#8345ED" Offset="0.7"/>
        <GradientStop Color="#BA45ED" Offset="1"/>
    </LinearGradientBrush>
    
    <!-- Sidebar Gradient (Vertical) -->
    <LinearGradientBrush x:Key="MTM_Shared_Logic.SidebarGradientBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#2D1B69" Offset="0"/>
        <GradientStop Color="#4B45ED" Offset="1"/>
    </LinearGradientBrush>
    
    <!-- Card Hover Gradient -->
    <LinearGradientBrush x:Key="MTM_Shared_Logic.CardHoverGradientBrush" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#4B45ED" Offset="0"/>
        <GradientStop Color="#BA45ED" Offset="1"/>
    </LinearGradientBrush>
    
    <!-- ================================================================== -->
    <!-- UI LAYOUT COLORS                                                  -->
    <!-- ================================================================== -->
    
    <!-- Background Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.AppBackgroundBrush" Color="#F8F7FF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ContentBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SidebarBackgroundBrush" Color="#2D1B69"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.HeaderBackgroundBrush" Color="#4B45ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.FooterBackgroundBrush" Color="#2D1B69"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.StatusBarBackgroundBrush" Color="#2D1B69"/>
    
    <!-- Card and Panel Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.CardBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PanelBackgroundBrush" Color="#FAFAFA"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ModalBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.OverlayBackgroundBrush" Color="#80000000"/>
    
    <!-- Border Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.BorderBrush" Color="#E8E5FF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.BorderDarkBrush" Color="#D4C4F7"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SeparatorBrush" Color="#E8E5FF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.OutlineBrush" Color="#B594ED"/>
    
    <!-- ================================================================== -->
    <!-- TEXT COLORS                                                        -->
    <!-- ================================================================== -->
    
    <!-- Primary Text -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryTextBrush" Color="#2D1B69"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryTextBrush" Color="#666666"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.TertiaryTextBrush" Color="#999999"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.LightTextBrush" Color="#FFFFFF"/>
    
    <!-- Specialized Text -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.AccentTextBrush" Color="#4B45ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.LinkTextBrush" Color="#4574ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.DisabledTextBrush" Color="#CCCCCC"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PlaceholderTextBrush" Color="#AAAAAA"/>
    
    <!-- Status Text Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.SuccessTextBrush" Color="#28A745"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.WarningTextBrush" Color="#FFC107"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ErrorTextBrush" Color="#DC3545"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.InfoTextBrush" Color="#17A2B8"/>
    
    <!-- ================================================================== -->
    <!-- SEMANTIC COLORS                                                   -->
    <!-- ================================================================== -->
    
    <!-- Success Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.SuccessBrush" Color="#28A745"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SuccessLightBrush" Color="#D4F6D4"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SuccessHoverBrush" Color="#218838"/>
    
    <!-- Warning Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.WarningBrush" Color="#FFC107"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.WarningLightBrush" Color="#FFF3CD"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.WarningHoverBrush" Color="#E0A800"/>
    
    <!-- Error/Danger Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.ErrorBrush" Color="#DC3545"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ErrorLightBrush" Color="#F8D7DA"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ErrorHoverBrush" Color="#C82333"/>
    
    <!-- Info Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.InfoBrush" Color="#17A2B8"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.InfoLightBrush" Color="#D1ECF1"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.InfoHoverBrush" Color="#138496"/>
    
    <!-- ================================================================== -->
    <!-- COMPONENT-SPECIFIC COLORS                                         -->
    <!-- ================================================================== -->
    
    <!-- Button Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.ButtonPrimaryBrush" Color="#4B45ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ButtonSecondaryBrush" Color="#8345ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ButtonDangerBrush" Color="#DC3545"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ButtonSuccessBrush" Color="#28A745"/>
    
    <!-- Input Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.InputBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.InputBorderBrush" Color="#E8E5FF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.InputFocusBrush" Color="#4B45ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.InputErrorBrush" Color="#DC3545"/>
    
    <!-- Selection Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.SelectionBrush" Color="#4B45ED"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SelectionLightBrush" Color="#E8E5FF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.HighlightBrush" Color="#B594ED"/>
    
    <!-- ================================================================== -->
    <!-- LEGACY COMPATIBILITY ALIASES                                      -->
    <!-- ================================================================== -->
    <!-- These maintain backward compatibility with existing code -->
    
    <SolidColorBrush x:Key="PrimaryBrush" Color="#4B45ED"/>
    <SolidColorBrush x:Key="AccentBrush" Color="#4B45ED"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="#8345ED"/>
    <SolidColorBrush x:Key="MagentaAccentBrush" Color="#BA45ED"/>
    <SolidColorBrush x:Key="BlueAccentBrush" Color="#4574ED"/>
    <SolidColorBrush x:Key="PinkAccentBrush" Color="#ED45E7"/>
    <SolidColorBrush x:Key="LightPurpleBrush" Color="#B594ED"/>
    <SolidColorBrush x:Key="PrimaryDarkBrush" Color="#2D1B69"/>
    <SolidColorBrush x:Key="SecondaryDarkBrush" Color="#1A1B1C"/>
    <SolidColorBrush x:Key="AccentHoverBrush" Color="#BA45ED"/>
    <SolidColorBrush x:Key="AccentPressedBrush" Color="#8345ED"/>
    <SolidColorBrush x:Key="AccentDisabledBrush" Color="#B594ED"/>
    <SolidColorBrush x:Key="SidebarBackgroundBrush" Color="#2D1B69"/>
    <SolidColorBrush x:Key="ContentBackgroundBrush" Color="#F8F7FF"/>
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="StatusBarBackgroundBrush" Color="#2D1B69"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#E8E5FF"/>
    <SolidColorBrush x:Key="PrimaryTextBrush" Color="#2D1B69"/>
    <SolidColorBrush x:Key="SecondaryTextBrush" Color="#666666"/>
    <SolidColorBrush x:Key="LightTextBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="#2D1B69"/>
    
    <!-- Legacy Gradients -->
    <LinearGradientBrush x:Key="PrimaryGradientBrush" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#4B45ED" Offset="0"/>
        <GradientStop Color="#BA45ED" Offset="0.5"/>
        <GradientStop Color="#8345ED" Offset="1"/>
    </LinearGradientBrush>
    
    <LinearGradientBrush x:Key="HeroGradientBrush" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#4574ED" Offset="0"/>
        <GradientStop Color="#4B45ED" Offset="0.3"/>
        <GradientStop Color="#8345ED" Offset="0.7"/>
        <GradientStop Color="#BA45ED" Offset="1"/>
    </LinearGradientBrush>

</ResourceDictionary>
