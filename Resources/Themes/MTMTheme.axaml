<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- ================================================================== -->
	<!-- MTM Default Theme - Maximum Robustness Edition                   -->
	<!-- Complete premium theme with all visual effects and interactions   -->
	<!-- ================================================================== -->

	<!-- Core MTM Default Brand Colors (Windows 11 Style) -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryAction" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryAction" Color="#106EBE"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Warning" Color="#FFB900"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Status" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Critical" Color="#D13438"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Highlight" Color="#005A9E"/>

	<!-- Extended Palette for Enhanced Visual Effects -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.DarkNavigation" Color="#323130"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.CardBackground" Color="#F3F2F1"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.HoverBackground" Color="#EDEBE9"/>

	<!-- Advanced Interactive State Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryHoverBrush" Color="#106EBE"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryHoverBrush" Color="#005A9E"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.MagentaHoverBrush" Color="#FFB900"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryPressedBrush" Color="#005A9E"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryPressedBrush" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.MagentaPressedBrush" Color="#D13438"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDisabledBrush" Color="#C8C6C4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryDisabledBrush" Color="#F3F2F1"/>

	<!-- Premium Gradient Brushes for Visual Excellence -->
	<LinearGradientBrush x:Key="MTM_Shared_Logic.PrimaryGradientBrush" StartPoint="0,0" EndPoint="1,0">
		<GradientStop Color="#0078D4" Offset="0"/>
		<GradientStop Color="#106EBE" Offset="0.5"/>
		<GradientStop Color="#005A9E" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="MTM_Shared_Logic.HeroGradientBrush" StartPoint="0,0" EndPoint="1,1">
		<GradientStop Color="#323130" Offset="0"/>
		<GradientStop Color="#0078D4" Offset="0.3"/>
		<GradientStop Color="#106EBE" Offset="0.7"/>
		<GradientStop Color="#005A9E" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="MTM_Shared_Logic.SidebarGradientBrush" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#323130" Offset="0"/>
		<GradientStop Color="#0078D4" Offset="0.6"/>
		<GradientStop Color="#005A9E" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="MTM_Shared_Logic.CardHoverGradientBrush" StartPoint="0,0" EndPoint="1,0">
		<GradientStop Color="#EDEBE9" Offset="0"/>
		<GradientStop Color="#F3F2F1" Offset="0.5"/>
		<GradientStop Color="#E1DFDD" Offset="1" />
	</LinearGradientBrush>

	<RadialGradientBrush x:Key="MTM_Shared_Logic.AccentRadialBrush" Center="0.5,0.5" Radius="1">
		<GradientStop Color="#0078D4" Offset="0"/>
		<GradientStop Color="#106EBE" Offset="0.7"/>
		<GradientStop Color="#323130" Offset="1"/>
	</RadialGradientBrush>

	<!-- Comprehensive UI Layout Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.MainBackground" Color="#FAFAFA"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ContentAreas" Color="#FFFFFF"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SidebarDark" Color="#323130"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PageHeaders" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.FooterBackgroundBrush" Color="#323130"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.StatusBarBackgroundBrush" Color="#323130"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.CardBackgroundBrush" Color="#FFFFFF"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PanelBackgroundBrush" Color="#F5F5F5"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderBrush" Color="#E1DFDD"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderDarkBrush" Color="#C8C6C4"/>

	<!-- Advanced Border and Shadow Effects -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderLightBrush" Color="#F3F2F1"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderAccentBrush" Color="#DEECF9"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ShadowBrush" Color="#33000000"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.DropShadowBrush" Color="#66323130"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.GlowBrush" Color="#800078D4"/>

	<!-- Comprehensive Text Color System -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.HeadingText" Color="#323130"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BodyText" Color="#605E5C"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.TertiaryTextBrush" Color="#8A8886"/>
	
	<SolidColorBrush x:Key="MTM_Shared_Logic.InteractiveText" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.LinkTextBrush" Color="#106EBE"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.MutedTextBrush" Color="#C8C6C4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.HighlightTextBrush" Color="#005A9E"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PlaceholderTextBrush" Color="#A19F9D"/>

	<!-- Enhanced Semantic Colors with Variants (KEEP EXACT COLORS) -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessBrush" Color="#4CAF50"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessLightBrush" Color="#C8E6C9"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessDarkBrush" Color="#2E7D32"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.WarningBrush" Color="#FF9800"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.WarningLightBrush" Color="#FFE0B2"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.WarningDarkBrush" Color="#F57C00"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorBrush" Color="#F44336"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorLightBrush" Color="#FFCDD2"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorDarkBrush" Color="#D32F2F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InfoBrush" Color="#2196F3"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InfoLightBrush" Color="#BBDEFB"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InfoDarkBrush" Color="#1976D2"/>

	<!-- Specialized Theme Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized1" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized2" Color="#106EBE"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized3" Color="#FFB900"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized4" Color="#323130"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized5" Color="#C8C6C4"/>

	<!-- Advanced State Management Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.FocusBrush" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SelectionBrush" Color="#80DEECF9"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ActiveBrush" Color="#106EBE"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InactiveBrush" Color="#C8C6C4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.LoadingBrush" Color="#E1DFDD"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ProcessingBrush" Color="#FFB900"/>



	<!-- Comprehensive Design.PreviewWith Showcase -->
	<Design.PreviewWith>
		<Border Padding="20" Background="{DynamicResource MTM_Shared_Logic.MainBackground}">
			<ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
				<StackPanel Spacing="24">
					<!-- Premium Hero Section with Gradient -->
					<Border Background="{DynamicResource MTM_Shared_Logic.HeroGradientBrush}"
							CornerRadius="12"
							Padding="32"
							Margin="0,0,0,8">
						<StackPanel HorizontalAlignment="Center">
							<TextBlock Text="MTM Industrial Amber Theme"
									   FontSize="32"
									   FontWeight="Bold"
									   Foreground="{DynamicResource MTM_Shared_Logic.OverlayTextBrush}"
									   HorizontalAlignment="Center"/>
							<TextBlock Text="Maximum Robustness Edition - Premium Industrial Aesthetic"
									   FontSize="16"
									   Foreground="{DynamicResource MTM_Shared_Logic.OverlayTextBrush}"
									   HorizontalAlignment="Center"
									   Margin="0,8,0,0"
									   Opacity="0.9"/>
							<StackPanel Orientation="Horizontal" Spacing="16" HorizontalAlignment="Center" Margin="0,16,0,0">
								<Border Background="{DynamicResource MTM_Shared_Logic.OverlayTextBrush}" 
										CornerRadius="16" Padding="12,6">
									<TextBlock Text="Industrial Grade" 
											   Foreground="{DynamicResource MTM_Shared_Logic.DarkNavigation}" 
											   FontWeight="SemiBold" FontSize="12"/>
								</Border>
								<Border Background="{DynamicResource MTM_Shared_Logic.OverlayTextBrush}" 
										CornerRadius="16" Padding="12,6">
									<TextBlock Text="Premium Effects" 
											   Foreground="{DynamicResource MTM_Shared_Logic.DarkNavigation}" 
											   FontWeight="SemiBold" FontSize="12"/>
								</Border>
								<Border Background="{DynamicResource MTM_Shared_Logic.OverlayTextBrush}" 
										CornerRadius="16" Padding="12,6">
									<TextBlock Text="Complete Coverage" 
											   Foreground="{DynamicResource MTM_Shared_Logic.DarkNavigation}" 
											   FontWeight="SemiBold" FontSize="12"/>
								</Border>
							</StackPanel>
						</StackPanel>
					</Border>

					<!-- Complete Color Palette Showcase -->
					<Border Background="{DynamicResource MTM_Shared_Logic.ContentAreas}"
							BorderBrush="{DynamicResource MTM_Shared_Logic.BorderAccentBrush}"
							BorderThickness="2"
							CornerRadius="12"
							Padding="24">
						<StackPanel>
							<TextBlock Text="Complete Industrial Amber Palette"
									   FontSize="20"
									   FontWeight="SemiBold"
									   Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}"
									   Margin="0,0,0,16"/>

							<!-- Core Action Colors -->
							<TextBlock Text="Core Action Colors" FontWeight="Medium" Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}" Margin="0,0,0,8"/>
							<Grid ColumnDefinitions="*,*,*,*" RowSpacing="8" ColumnSpacing="8" Margin="0,0,0,16">
								<Border Grid.Column="0" Background="{DynamicResource MTM_Shared_Logic.PrimaryAction}" Height="60" CornerRadius="6">
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
										<TextBlock Text="Primary Action" FontSize="11" FontWeight="Bold" Foreground="White" HorizontalAlignment="Center"/>
										<TextBlock Text="#FF8F00" FontSize="9" Foreground="White" HorizontalAlignment="Center"/>
									</StackPanel>
								</Border>
								<Border Grid.Column="1" Background="{DynamicResource MTM_Shared_Logic.SecondaryAction}" Height="60" CornerRadius="6">
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
										<TextBlock Text="Secondary Action" FontSize="11" FontWeight="Bold" Foreground="White" HorizontalAlignment="Center"/>
										<TextBlock Text="#FFB300" FontSize="9" Foreground="White" HorizontalAlignment="Center"/>
									</StackPanel>
								</Border>
								<Border Grid.Column="2" Background="{DynamicResource MTM_Shared_Logic.Warning}" Height="60" CornerRadius="6">
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
										<TextBlock Text="Warning" FontSize="11" FontWeight="Bold" Foreground="White" HorizontalAlignment="Center"/>
										<TextBlock Text="#FFC107" FontSize="9" Foreground="White" HorizontalAlignment="Center"/>
									</StackPanel>
								</Border>
								<Border Grid.Column="3" Background="{DynamicResource MTM_Shared_Logic.Critical}" Height="60" CornerRadius="6">
									<StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
										<TextBlock Text="Critical" FontSize="11" FontWeight="Bold" Foreground="White" HorizontalAlignment="Center"/>
										<TextBlock Text="#FF6F00" FontSize="9" Foreground="White" HorizontalAlignment="Center"/>
									</StackPanel>
								</Border>
							</Grid>

							<!-- Gradient Showcase -->
							<TextBlock Text="Premium Gradient Effects" FontWeight="Medium" Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}" Margin="0,0,0,8"/>
							<Grid ColumnDefinitions="*,*" RowSpacing="8" ColumnSpacing="12" Margin="0,0,0,16">
								<Border Grid.Column="0" Background="{DynamicResource MTM_Shared_Logic.PrimaryGradientBrush}" Height="60" CornerRadius="6">
									<TextBlock Text="Primary Gradient" FontSize="12" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Column="1" Background="{DynamicResource MTM_Shared_Logic.SidebarGradientBrush}" Height="60" CornerRadius="6">
									<TextBlock Text="Sidebar Gradient" FontSize="12" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
							</Grid>

							<!-- Specialized Amber Colors -->
							<TextBlock Text="Specialized Industrial Amber Collection" FontWeight="Medium" Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}" Margin="0,0,0,8"/>
							<Grid ColumnDefinitions="*,*,*,*,*" RowSpacing="8" ColumnSpacing="6" Margin="0,0,0,16">
								<Border Grid.Column="0" Background="{DynamicResource MTM_Shared_Logic.Specialized1}" Height="50" CornerRadius="4">
									<TextBlock Text="Industrial" FontSize="10" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Column="1" Background="{DynamicResource MTM_Shared_Logic.Specialized2}" Height="50" CornerRadius="4">
									<TextBlock Text="Burnished" FontSize="10" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Column="2" Background="{DynamicResource MTM_Shared_Logic.Specialized3}" Height="50" CornerRadius="4">
									<TextBlock Text="Molten" FontSize="10" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Column="3" Background="{DynamicResource MTM_Shared_Logic.Specialized4}" Height="50" CornerRadius="4">
									<TextBlock Text="Deep" FontSize="10" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Column="4" Background="{DynamicResource MTM_Shared_Logic.Specialized5}" Height="50" CornerRadius="4">
									<TextBlock Text="Crystal" FontSize="10" FontWeight="Bold" Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
							</Grid>

							<!-- Semantic Color System -->
							<TextBlock Text="Enhanced Semantic System" FontWeight="Medium" Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}" Margin="0,0,0,8"/>
							<Grid ColumnDefinitions="*,*,*,*" RowSpacing="8" ColumnSpacing="8">
								<Border Grid.Column="0" Background="{DynamicResource MTM_Shared_Logic.SuccessBrush}" Height="50" CornerRadius="4">
									<TextBlock Text="Success System" FontSize="10" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Column="1" Background="{DynamicResource MTM_Shared_Logic.WarningBrush}" Height="50" CornerRadius="4">
									<TextBlock Text="Warning System" FontSize="10" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Column="2" Background="{DynamicResource MTM_Shared_Logic.ErrorBrush}" Height="50" CornerRadius="4">
									<TextBlock Text="Error System" FontSize="10" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Column="3" Background="{DynamicResource MTM_Shared_Logic.InfoBrush}" Height="50" CornerRadius="4">
									<TextBlock Text="Info System" FontSize="10" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
							</Grid>
						</StackPanel>
					</Border>

					<!-- Advanced Interactive States Demo -->
					<Border Background="{DynamicResource MTM_Shared_Logic.ContentAreas}"
							BorderBrush="{DynamicResource MTM_Shared_Logic.BorderBrush}"
							BorderThickness="2"
							CornerRadius="12"
							Padding="24">
						<StackPanel>
							<TextBlock Text="Advanced Interactive States"
									   FontSize="18"
									   FontWeight="SemiBold"
									   Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}"
									   Margin="0,0,0,16"/>

							<!-- State Management Colors -->
							<Grid ColumnDefinitions="*,*,*" RowDefinitions="Auto,Auto" ColumnSpacing="12" RowSpacing="12" Margin="0,0,0,16">
								<Border Grid.Row="0" Grid.Column="0" Background="{DynamicResource MTM_Shared_Logic.FocusBrush}" Height="45" CornerRadius="6">
									<TextBlock Text="Focus State" FontSize="12" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Row="0" Grid.Column="1" Background="{DynamicResource MTM_Shared_Logic.ActiveBrush}" Height="45" CornerRadius="6">
									<TextBlock Text="Active State" FontSize="12" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Row="0" Grid.Column="2" Background="{DynamicResource MTM_Shared_Logic.ProcessingBrush}" Height="45" CornerRadius="6">
									<TextBlock Text="Processing State" FontSize="12" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Row="1" Grid.Column="0" Background="{DynamicResource MTM_Shared_Logic.SelectionBrush}" BorderBrush="{DynamicResource MTM_Shared_Logic.PrimaryAction}" BorderThickness="2" Height="45" CornerRadius="6">
									<TextBlock Text="Selection State" FontSize="12" FontWeight="Bold" Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Row="1" Grid.Column="1" Background="{DynamicResource MTM_Shared_Logic.LoadingBrush}" Height="45" CornerRadius="6">
									<TextBlock Text="Loading State" FontSize="12" FontWeight="Bold" Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
								<Border Grid.Row="1" Grid.Column="2" Background="{DynamicResource MTM_Shared_Logic.InactiveBrush}" Height="45" CornerRadius="6">
									<TextBlock Text="Inactive State" FontSize="12" FontWeight="Bold" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
								</Border>
							</Grid>

							<!-- Premium Button Demonstrations -->
							<TextBlock Text="Premium Button Collection" FontWeight="Medium" Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}" Margin="0,0,0,12"/>
							<StackPanel Orientation="Horizontal" Spacing="16" Margin="0,0,0,16">
								<Button Content="Primary Action"
										Background="{DynamicResource MTM_Shared_Logic.PrimaryAction}"
										Foreground="{DynamicResource MTM_Shared_Logic.OverlayTextBrush}"
										Padding="16,10"
										CornerRadius="6"
										BorderThickness="0"
										FontWeight="SemiBold"/>
								<Button Content="Secondary Action"
										Background="{DynamicResource MTM_Shared_Logic.SecondaryAction}"
										Foreground="{DynamicResource MTM_Shared_Logic.OverlayTextBrush}"
										Padding="16,10"
										CornerRadius="6"
										BorderThickness="0"
										FontWeight="SemiBold"/>
								<Button Content="Gradient Button"
										Background="{DynamicResource MTM_Shared_Logic.PrimaryGradientBrush}"
										Foreground="{DynamicResource MTM_Shared_Logic.OverlayTextBrush}"
										Padding="16,10"
										CornerRadius="6"
										BorderThickness="0"
										FontWeight="SemiBold"/>
								<Button Content="Outline Style"
										Background="Transparent"
										Foreground="{DynamicResource MTM_Shared_Logic.PrimaryAction}"
										BorderBrush="{DynamicResource MTM_Shared_Logic.PrimaryAction}"
										Padding="16,10"
										CornerRadius="6"
										BorderThickness="2"
										FontWeight="SemiBold"/>
							</StackPanel>
						</StackPanel>
					</Border>

					<!-- Typography and Text System -->
					<Border Background="{DynamicResource MTM_Shared_Logic.ContentAreas}"
							BorderBrush="{DynamicResource MTM_Shared_Logic.BorderDarkBrush}"
							BorderThickness="2"
							CornerRadius="12"
							Padding="24">
						<StackPanel>
							<TextBlock Text="Complete Typography System"
									   FontSize="18"
									   FontWeight="SemiBold"
									   Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}"
									   Margin="0,0,0,16"/>

							<TextBlock Text="Hero Heading - Industrial Excellence"
									   FontSize="36"
									   FontWeight="Bold"
									   Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}"
									   Margin="0,0,0,12"/>
							<TextBlock Text="Primary Heading - System Integration"
									   FontSize="28"
									   FontWeight="SemiBold"
									   Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}"
									   Margin="0,0,0,10"/>
							<TextBlock Text="Secondary Heading - Component Overview"
									   FontSize="20"
									   FontWeight="Medium"
									   Foreground="{DynamicResource MTM_Shared_Logic.InteractiveText}"
									   Margin="0,0,0,8"/>
							<TextBlock Text="Body text provides comprehensive information about the MTM Industrial Amber theme system. This theme has been engineered for maximum robustness and visual excellence in manufacturing environments. Every color, gradient, and interactive state has been carefully crafted."
									   FontSize="14"
									   Foreground="{DynamicResource MTM_Shared_Logic.BodyText}"
									   TextWrapping="Wrap"
									   Margin="0,0,0,8"/>
							<TextBlock Text="Tertiary text provides additional context and supporting information."
									   FontSize="13"
									   Foreground="{DynamicResource MTM_Shared_Logic.TertiaryTextBrush}"
									   Margin="0,0,0,8"/>
							<TextBlock Text="Interactive text link for navigation and actions"
									   FontSize="14"
									   Foreground="{DynamicResource MTM_Shared_Logic.LinkTextBrush}"
									   FontWeight="Medium"
									   Margin="0,0,0,8"/>
							<TextBlock Text="Highlighted text for emphasis and call-outs"
									   FontSize="14"
									   Foreground="{DynamicResource MTM_Shared_Logic.HighlightTextBrush}"
									   FontWeight="SemiBold"
									   Margin="0,0,0,8"/>
							<TextBlock Text="Muted text for less important information"
									   FontSize="12"
									   Foreground="{DynamicResource MTM_Shared_Logic.MutedTextBrush}"
									   Margin="0,0,0,8"/>
						</StackPanel>
					</Border>

					<!-- Advanced Component Showcase -->
					<Border Background="{DynamicResource MTM_Shared_Logic.PanelBackgroundBrush}"
							BorderBrush="{DynamicResource MTM_Shared_Logic.BorderLightBrush}"
							BorderThickness="1"
							CornerRadius="12"
							Padding="24">
						<StackPanel>
							<TextBlock Text="Advanced Manufacturing Components"
									   FontSize="18"
									   FontWeight="SemiBold"
									   Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}"
									   Margin="0,0,0,16"/>

							<Grid ColumnDefinitions="*,*,*" ColumnSpacing="16" RowSpacing="16">
								<!-- Production Card -->
								<Border Grid.Column="0"
										Background="{DynamicResource MTM_Shared_Logic.CardBackgroundBrush}"
										BorderBrush="{DynamicResource MTM_Shared_Logic.BorderAccentBrush}"
										BorderThickness="2"
										CornerRadius="8"
										Padding="20">
									<StackPanel>
										<TextBlock Text="ðŸ­ Production Status" FontWeight="Bold" FontSize="16" Foreground="{DynamicResource MTM_Shared_Logic.HeadingText}" Margin="0,0,0,8"/>
										<TextBlock Text="Operation: 110" Foreground="{DynamicResource MTM_Shared_Logic.BodyText}" Margin="0,0,0,4"/>
										<TextBlock Text="Parts: 1,247 Active" Foreground="{DynamicResource MTM_Shared_Logic.InteractiveText}" FontWeight="Medium" Margin="0,0,0,4"/>
										<TextBlock Text="Efficiency: 97.3%" Foreground="{DynamicResource MTM_Shared_Logic.SuccessBrush}" FontWeight="SemiBold"/>
									</StackPanel>
								</Border>

								<!-- Quality Card -->
								<Border Grid.Column="1"
										Background="{DynamicResource MTM_Shared_Logic.CardHoverGradientBrush}"
										BorderBrush="{DynamicResource MTM_Shared_Logic.BorderDarkBrush}"
										BorderThickness="2"
										CornerRadius="8"
										Padding="20">
									<StackPanel>
