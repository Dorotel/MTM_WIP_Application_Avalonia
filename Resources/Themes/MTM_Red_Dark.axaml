<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- ================================================================== -->
	<!-- MTM Dark Red Default Theme - Maximum Robustness Edition          -->
	<!-- Complete premium theme with all visual effects and interactions   -->
	<!-- ================================================================== -->

	<!-- Core MTM Dark Red Default Brand Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryAction" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryAction" Color="#EF9A9A"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Warning" Color="#FFB74D"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Status" Color="#F44336"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Critical" Color="#B71C1C"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Highlight" Color="#C62828"/>

	<!-- Extended Palette for Enhanced Visual Effects -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.DarkNavigation" Color="#B71C1C"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.CardBackground" Color="#2E2E2E"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.HoverBackground" Color="#383838"/>

	<!-- Advanced Interactive State Colors -->
	<!-- Overlay Text Brush for Universal Contrast -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.OverlayTextBrush" Color="#FFFFFFFF"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryHoverBrush" Color="#EF5350"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryHoverBrush" Color="#106EBE"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.MagentaHoverBrush" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryPressedBrush" Color="#F44336"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryPressedBrush" Color="#005A9E"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.MagentaPressedBrush" Color="#B71C1C"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDisabledBrush" Color="#5A5A5A"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryDisabledBrush" Color="#2E2E2E"/>

	<!-- Premium Gradient Brushes for Visual Excellence -->
	<LinearGradientBrush x:Key="MTM_Shared_Logic.PrimaryGradientBrush" StartPoint="0,0" EndPoint="1,0">
		<GradientStop Color="#C62828" Offset="0"/>
		<GradientStop Color="#0078D4" Offset="0.5"/>
		<GradientStop Color="#EF5350" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="MTM_Shared_Logic.HeroGradientBrush" StartPoint="0,0" EndPoint="1,1">
		<GradientStop Color="#B71C1C" Offset="0"/>
		<GradientStop Color="#C62828" Offset="0.3"/>
		<GradientStop Color="#0078D4" Offset="0.7"/>
		<GradientStop Color="#EF5350" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="MTM_Shared_Logic.SidebarGradientBrush" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#B71C1C" Offset="0"/>
		<GradientStop Color="#C62828" Offset="0.6"/>
		<GradientStop Color="#F44336" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="MTM_Shared_Logic.CardHoverGradientBrush" StartPoint="0,0" EndPoint="1,0">
		<GradientStop Color="#383838" Offset="0"/>
		<GradientStop Color="#2E2E2E" Offset="0.5"/>
		<GradientStop Color="#C62828" Offset="1" />
	</LinearGradientBrush>

	<RadialGradientBrush x:Key="MTM_Shared_Logic.AccentRadialBrush" Center="0.5,0.5" RadiusX="100%" RadiusY="100%">
		<GradientStop Color="#EF5350" Offset="0"/>
		<GradientStop Color="#C62828" Offset="0.7"/>
		<GradientStop Color="#B71C1C" Offset="1"/>
	</RadialGradientBrush>

	<!-- Comprehensive UI Layout Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.MainBackground" Color="#212121"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ContentAreas" Color="#2E2E2E"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SidebarDark" Color="#B71C1C"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PageHeaders" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.FooterBackgroundBrush" Color="#B71C1C"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.StatusBarBackgroundBrush" Color="#B71C1C"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.CardBackgroundBrush" Color="#2E2E2E"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PanelBackgroundBrush" Color="#1A1A1A"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderBrush" Color="#404040"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderDarkBrush" Color="#C62828"/>

	<!-- Advanced Border and Shadow Effects -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderLightBrush" Color="#4A4A4A"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BorderAccentBrush" Color="#EF9A9A"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ShadowBrush" Color="#80000000"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.DropShadowBrush" Color="#66B71C1C"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.GlowBrush" Color="#80E57373"/>

	<!-- Comprehensive Text Color System -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.HeadingText" Color="#E0E0E0"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.BodyText" Color="#E0E0E0"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.TertiaryTextBrush" Color="#666666"/>
	
	<SolidColorBrush x:Key="MTM_Shared_Logic.InteractiveText" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.LinkTextBrush" Color="#EF5350"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.MutedTextBrush" Color="#888888"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.HighlightTextBrush" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.PlaceholderTextBrush" Color="#666666"/>

	<!-- Enhanced Semantic Colors with Variants (KEEP EXACT COLORS) -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessBrush" Color="#2E7D32"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessLightBrush" Color="#C8E6C9"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessDarkBrush" Color="#2E7D32"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.WarningBrush" Color="#E65100"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.WarningLightBrush" Color="#FFE0B2"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.WarningDarkBrush" Color="#F57C00"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorBrush" Color="#F44336"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorLightBrush" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorDarkBrush" Color="#D32F2F"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InfoBrush" Color="#2196F3"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InfoLightBrush" Color="#BBDEFB"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InfoDarkBrush" Color="#1976D2"/>
	<!-- Transaction Type Colors for History Panel (WCAG 2.1 AA Compliant) -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionInBrush" Color="#198754"/>      <!-- Green for IN - 4.5:1 contrast -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionOutBrush" Color="#DC3545"/>     <!-- Red for OUT - 4.5:1 contrast -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionTransferBrush" Color="#B85500"/> <!-- Orange for Transfer - 4.5:1 contrast -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionInLightBrush" Color="#D1E7DD"/>  <!-- Light Green -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionOutLightBrush" Color="#F8D7DA"/> <!-- Light Red -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionTransferLightBrush" Color="#FFF3CD"/> <!-- Light Orange -->



	<!-- Specialized Theme Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized1" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized2" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized3" Color="#EF5350"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized4" Color="#B71C1C"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized5" Color="#C62828"/>

	<!-- Advanced State Management Colors -->
	<SolidColorBrush x:Key="MTM_Shared_Logic.FocusBrush" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.SelectionBrush" Color="#80EF5350"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ActiveBrush" Color="#0078D4"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.InactiveBrush" Color="#5A5A5A"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.LoadingBrush" Color="#C62828"/>
	<SolidColorBrush x:Key="MTM_Shared_Logic.ProcessingBrush" Color="#EF5350"/>


<SolidColorBrush x:Key="MTM_Shared_Logic.InputBackground" Color="#FFFFFF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InputBorder" Color="#CED4DA"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InputFocusBorder" Color="#0056B3"/>
</ResourceDictionary>


