<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- ================================================================== -->
    <!-- MTM WIP Application Focus Teal Theme                              -->
    <!-- ================================================================== -->
    
    <!-- Core MTM Teal Brand Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryBrush" Color="#00ACC1"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryBrush" Color="#26C6DA"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.MagentaAccentBrush" Color="#4DD0E1"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.BlueAccentBrush" Color="#0097A7"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PinkAccentBrush" Color="#00BCD4"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.LightPurpleBrush" Color="#80DEEA"/>
    
    <!-- Extended Palette -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDarkBrush" Color="#006064"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryLightBrush" Color="#E0F2F1"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryExtraLightBrush" Color="#F0FFFE"/>
    
    <!-- Interactive State Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryHoverBrush" Color="#00BCD4"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryHoverBrush" Color="#4DD0E1"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.MagentaHoverBrush" Color="#80DEEA"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryPressedBrush" Color="#0097A7"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryPressedBrush" Color="#00ACC1"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.MagentaPressedBrush" Color="#26C6DA"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDisabledBrush" Color="#80DEEA"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryDisabledBrush" Color="#B2EBF2"/>
    
    <!-- UI Layout Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.AppBackgroundBrush" Color="#F0FFFE"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ContentBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SidebarBackgroundBrush" Color="#006064"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.HeaderBackgroundBrush" Color="#00ACC1"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.FooterBackgroundBrush" Color="#006064"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.StatusBarBackgroundBrush" Color="#006064"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.CardBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PanelBackgroundBrush" Color="#FAFAFA"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.BorderBrush" Color="#E0F2F1"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.BorderDarkBrush" Color="#B2EBF2"/>
    
    <!-- Text Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryTextBrush" Color="#006064"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryTextBrush" Color="#666666"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.TertiaryTextBrush" Color="#999999"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.LightTextBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.AccentTextBrush" Color="#00ACC1"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.LinkTextBrush" Color="#0097A7"/>
    
    <!-- Semantic Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.SuccessBrush" Color="#4CAF50"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.WarningBrush" Color="#FF9800"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ErrorBrush" Color="#F44336"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.InfoBrush" Color="#00BCD4"/>

</ResourceDictionary>