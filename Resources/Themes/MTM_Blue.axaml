<SolidColorBrush x:Key="MTM_Shared_Logic.CardBackgroundBrush" Color="#F8FBFF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.PanelBackgroundBrush" Color="#ADE1FB"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.SidebarDark" Color="#041D56"/>
