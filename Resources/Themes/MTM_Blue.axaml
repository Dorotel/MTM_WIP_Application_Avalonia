<!-- Gradient Brushes - Professional Blue Palette -->
<LinearGradientBrush x:Key="MTM_Shared_Logic.PrimaryGradientBrush" StartPoint="0,0" EndPoint="1,0">
	<GradientStop Color="#266CA9" Offset="0"/>
	<GradientStop Color="#0F2573" Offset="1"/>
</LinearGradientBrush>

<LinearGradientBrush x:Key="MTM_Shared_Logic.HeroGradientBrush" StartPoint="0,0" EndPoint="1,1">
	<GradientStop Color="#01082D" Offset="0"/>
	<GradientStop Color="#041D56" Offset="0.3"/>
	<GradientStop Color="#0F2573" Offset="0.6"/>
	<GradientStop Color="#266CA9" Offset="0.9"/>
	<GradientStop Color="#ADE1FB" Offset="1"/>
</LinearGradientBrush>

<LinearGradientBrush x:Key="MTM_Shared_Logic.SidebarGradientBrush" StartPoint="0,0" EndPoint="0,1">
	<GradientStop Color="#01082D" Offset="0"/>
	<GradientStop Color="#041D56" Offset="0.4"/>
	<GradientStop Color="#0F2573" Offset="1"/>
</LinearGradientBrush>

<LinearGradientBrush x:Key="MTM_Shared_Logic.CardHoverGradientBrush" StartPoint="0,0" EndPoint="1,0">
	<GradientStop Color="#E8F4FD" Offset="0"/>
	<GradientStop Color="#ADE1FB" Offset="1"/>
</LinearGradientBrush>

