<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
<!-- MTM Blue Theme - Performance Optimized WCAG 2.1 AA Compliant -->

<!-- Core Colors -->
<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryAction" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryAction" Color="#004085"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.Warning" Color="#B85500"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.Status" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.Critical" Color="#B71C1C"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.Highlight" Color="#003D6B"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.DarkNavigation" Color="#212529"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.CardBackground" Color="#F8F9FA"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.HoverBackground" Color="#E9ECEF"/>

<!-- Interactive States -->
<SolidColorBrush x:Key="MTM_Shared_Logic.OverlayTextBrush" Color="#000000"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryHoverBrush" Color="#004085"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryHoverBrush" Color="#003D6B"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.MagentaHoverBrush" Color="#B85500"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryPressedBrush" Color="#003D6B"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryPressedBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.MagentaPressedBrush" Color="#B71C1C"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDisabledBrush" Color="#ADB5BD"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryDisabledBrush" Color="#F8F9FA"/>

<!-- Simplified Gradients for Performance -->
<LinearGradientBrush x:Key="MTM_Shared_Logic.PrimaryGradientBrush" StartPoint="0,0" EndPoint="1,0">
	<GradientStop Color="#0056B3" Offset="0"/>
	<GradientStop Color="#003D6B" Offset="1"/>
</LinearGradientBrush>
<LinearGradientBrush x:Key="MTM_Shared_Logic.HeroGradientBrush" StartPoint="0,0" EndPoint="1,1">
	<GradientStop Color="#212529" Offset="0"/>
	<GradientStop Color="#003D6B" Offset="1"/>
</LinearGradientBrush>
<LinearGradientBrush x:Key="MTM_Shared_Logic.SidebarGradientBrush" StartPoint="0,0" EndPoint="0,1">
	<GradientStop Color="#212529" Offset="0"/>
	<GradientStop Color="#003D6B" Offset="1"/>
</LinearGradientBrush>
<LinearGradientBrush x:Key="MTM_Shared_Logic.CardHoverGradientBrush" StartPoint="0,0" EndPoint="1,0">
	<GradientStop Color="#E9ECEF" Offset="0"/>
	<GradientStop Color="#DEE2E6" Offset="1"/>
</LinearGradientBrush>

<!-- Layout Colors -->
<SolidColorBrush x:Key="MTM_Shared_Logic.MainBackground" Color="#FFFFFF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.ContentAreas" Color="#FFFFFF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.SidebarDark" Color="#212529"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.PageHeaders" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.FooterBackgroundBrush" Color="#212529"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.StatusBarBackgroundBrush" Color="#212529"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.CardBackgroundBrush" Color="#FFFFFF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.PanelBackgroundBrush" Color="#F8F9FA"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.BorderBrush" Color="#DEE2E6"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.BorderDarkBrush" Color="#ADB5BD"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.BorderLightBrush" Color="#F8F9FA"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.BorderAccentBrush" Color="#CCE5FF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.ShadowBrush" Color="#000000"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.DropShadowBrush" Color="#212529"/>

<!-- Typography -->
<SolidColorBrush x:Key="MTM_Shared_Logic.HeadingText" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.BodyText" Color="#666666"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.PlaceholderTextBrush" Color="#6C757D"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.TertiaryTextBrush" Color="#ADB5BD"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.DisabledTextBrush" Color="#ADB5BD"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InteractiveText" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.LinkTextBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.HyperlinkBrush" Color="#0056B3"/>

<!-- Form Controls -->
<SolidColorBrush x:Key="MTM_Shared_Logic.InputBackground" Color="#FFFFFF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InputBorder" Color="#CED4DA"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InputFocusBorder" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InputHoverBorder" Color="#004085"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InputDisabledBackground" Color="#F8F9FA"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InputErrorBorder" Color="#B71C1C"/>

<!-- Transaction Colors -->
<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionInBrush" Color="#2E7D32"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionOutBrush" Color="#DC3545"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionTransferBrush" Color="#B85500"/>

<!-- Success/Error States -->
<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessBrush" Color="#2E7D32"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.WarningBrush" Color="#B85500"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorBrush" Color="#B71C1C"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InfoBrush" Color="#0056B3"/>

<!-- Advanced Features -->
<SolidColorBrush x:Key="MTM_Shared_Logic.AccentBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.NotificationBackground" Color="#E3F2FD"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.TooltipBackground" Color="#212529"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.TooltipForeground" Color="#FFFFFF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.ModalOverlay" Color="#000000"/>

<!-- Additional Required Brushes -->
<SolidColorBrush x:Key="MTM_Shared_Logic.AccentRadialBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.ActiveBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorDarkBrush" Color="#B71C1C"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.ErrorLightBrush" Color="#FFCDD2"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.FocusBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.GlowBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.HighlightTextBrush" Color="#FFFFFF"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InactiveBrush" Color="#ADB5BD"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InfoDarkBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.InfoLightBrush" Color="#E3F2FD"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.LoadingBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.MutedTextBrush" Color="#ADB5BD"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.ProcessingBrush" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.SelectionBrush" Color="#E3F2FD"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized1" Color="#0056B3"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized2" Color="#004085"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized3" Color="#003D6B"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized4" Color="#212529"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.Specialized5" Color="#F8F9FA"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessDarkBrush" Color="#2E7D32"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.SuccessLightBrush" Color="#C8E6C9"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionInLightBrush" Color="#C8E6C9"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionOutLightBrush" Color="#FFCDD2"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.TransactionTransferLightBrush" Color="#FFE0B2"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.WarningDarkBrush" Color="#B85500"/>
<SolidColorBrush x:Key="MTM_Shared_Logic.WarningLightBrush" Color="#FFE0B2"/>

</ResourceDictionary>