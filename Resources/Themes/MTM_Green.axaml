<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- ================================================================== -->
    <!-- MTM WIP Application Success Green Theme                           -->
    <!-- ================================================================== -->
    
    <!-- Core MTM Green Brand Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryBrush" Color="#43A047"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryBrush" Color="#66BB6A"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.MagentaAccentBrush" Color="#81C784"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.BlueAccentBrush" Color="#388E3C"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PinkAccentBrush" Color="#4CAF50"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.LightPurpleBrush" Color="#A5D6A7"/>
    
    <!-- Extended Palette -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDarkBrush" Color="#1B5E20"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryLightBrush" Color="#E8F5E8"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryExtraLightBrush" Color="#F8FFF8"/>
    
    <!-- Interactive State Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryHoverBrush" Color="#4CAF50"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryHoverBrush" Color="#81C784"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.MagentaHoverBrush" Color="#A5D6A7"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryPressedBrush" Color="#388E3C"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryPressedBrush" Color="#43A047"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.MagentaPressedBrush" Color="#66BB6A"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryDisabledBrush" Color="#A5D6A7"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryDisabledBrush" Color="#C8E6C9"/>
    
    <!-- UI Layout Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.AppBackgroundBrush" Color="#F8FFF8"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ContentBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SidebarBackgroundBrush" Color="#1B5E20"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.HeaderBackgroundBrush" Color="#43A047"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.FooterBackgroundBrush" Color="#1B5E20"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.StatusBarBackgroundBrush" Color="#1B5E20"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.CardBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.PanelBackgroundBrush" Color="#FAFAFA"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.BorderBrush" Color="#E8F5E8"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.BorderDarkBrush" Color="#C8E6C9"/>
    
    <!-- Text Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.PrimaryTextBrush" Color="#1B5E20"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.SecondaryTextBrush" Color="#666666"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.TertiaryTextBrush" Color="#999999"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.LightTextBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.AccentTextBrush" Color="#43A047"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.LinkTextBrush" Color="#388E3C"/>
    
    <!-- Semantic Colors -->
    <SolidColorBrush x:Key="MTM_Shared_Logic.SuccessBrush" Color="#4CAF50"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.WarningBrush" Color="#FF9800"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.ErrorBrush" Color="#F44336"/>
    <SolidColorBrush x:Key="MTM_Shared_Logic.InfoBrush" Color="#2196F3"/>

</ResourceDictionary>